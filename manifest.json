{
    "manifest_version": 3,
    "name": "Auto Click Ion-Select Extension",
    "version": "1.0",
    "author":"DSP",
    "description": "A description of your extension",
    "permissions": [
        "webNavigation",
        "activeTab",
        "storage",
        "scripting"
      ],
    "host_permissions": ["*://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    
    "action": {
      "default_popup": "popup.html"
    }
  }
  